            <div class="team-circles">
                <div class="container-2">
                    <div class="title-section">
                        <div class="text-3">Services</div>
                    </div>
                    <div class="columns hero-content">
                        <div class="card">
                            <div class="image-wrapper-2">
                                <div class="image-wrapper-2">
                                    <img
                                        src="assets/Vectors-Wrapper.svg"
                                        loading="lazy"
                                        alt=""
                                        class="graphic"
                                    />
                                </div>
                            </div>
                            <div class="content-2">
                                <div class="info">
                                    <div class="services-headings">
                                        SERVERLESS CONSULTING
                                    </div>
                                </div>
